  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "eslint": "^6.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4"
  },
